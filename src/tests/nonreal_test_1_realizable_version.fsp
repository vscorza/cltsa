set Global_Alphabet = {}
set Controllable_Alphabet = {}

set Global_Signals	= {x1,x2,x3,y1}
set Output_Signals	= {y1}
order			= {x3 < x2 < x1 < y1}

/***********
**** THETA
***********/
ltl env theta Init_E in TEST	= (!x1 && !x2 && !x3).
ltl sys theta Init_S in TEST	= (!y1).
/***********
**** SYS RHO
***********/
ltl sys rho Sys_1 in TEST			= [](x3 <-> y1).
ltl sys rho Sys_2 in TEST			= [](X(x3 <-> y1)).
/***********
**** ENV RHO
***********/
ltl env rho Env_1 in TEST			= []((x1 && x2) <-> (X(x3))).
ltl env rho Env_2 in TEST			= [](!x2 <-> X(x2)).

ltl fluent G_1		= (y1).
ltl fluent A_1		= (x1).
ltl fluent A_2		= (x1 && x2).

/***********
**** SYNTHESIS
***********/
|f| ENV =(TEST).
|gr1| <{A_1, A_2}><{G_1}> STRAT = ENV.

STRAT_p = S_0,
S_0 = (<>-> S_1),
S_1 = (<>-> S_2),
S_2 = (x2.on-> S_3|<x1.on,x2.on>-> S_4),
S_3 = (<>-> S_5),
S_4 = (<>-> S_6),
S_5 = (x2.off-> S_13|<x1.on,x2.off>-> S_14),
S_6 = (<x1.off,x2.off,x3.on>-> S_7|<x2.off,x3.on>-> S_8),
S_7 = (y1.on-> S_9),
S_8 = (y1.on-> S_10),
S_9 = (<x2.on,x3.off>-> S_18|<x1.on,x2.on,x3.off>-> S_19),
S_10 = (<x1.off,x2.on,x3.off>-> S_11|<x2.on,x3.off>-> S_12),
S_11 = (y1.off-> S_5),
S_12 = (y1.off-> S_6),
S_13 = (<>-> S_2),
S_14 = (<>-> S_15),
S_15 = (<x1.off,x2.on>-> S_16|x2.on-> S_17),
S_16 = (<>-> S_5),
S_17 = (<>-> S_6),
S_18 = (y1.off-> S_5),
S_19 = (y1.off-> S_6).

// tests
equals FATAL_NON_REAL_REALIZABLE_VERSION(STRAT, STRAT_p).


